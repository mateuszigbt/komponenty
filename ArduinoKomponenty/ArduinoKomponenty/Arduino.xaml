<Window x:Class="ArduinoKomponenty.Arduino"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArduinoKomponenty"
        mc:Ignorable="d"
        Title="Arduino" Height="267" Width="261">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="27,32,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="121" Height="128" TextChanged="textBox_TextChanged" UndoLimit="0"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="167,32,0,0" VerticalAlignment="Top" Width="61" Height="40" Click="button_Click"/>

    </Grid>
</Window>
