<Window x:Class="ArduinoKomponenty.Arduino"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArduinoKomponenty"
        mc:Ignorable="d"
        Title="Arduino" Height="267" Width="261">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="27,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="91" Height="185" TextChanged="textBox_TextChanged" />
        <Button x:Name="button" Content="Uruchom" HorizontalAlignment="Left" Margin="145,32,0,0" VerticalAlignment="Top" Width="61" Height="41" Click="button_Click"/>
        <Button x:Name="button1" Content="Wyłącz" HorizontalAlignment="Left" Margin="145,91,0,0" VerticalAlignment="Top" Width="61" Height="41" Click="button1_Click"/>

    </Grid>
</Window>
